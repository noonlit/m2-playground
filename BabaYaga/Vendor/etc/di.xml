<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="baba_yaga_note_listing_data_source" xsi:type="string">
                    BabaYaga\Vendor\Model\ResourceModel\Vendor\Grid\Collection
                </item>
            </argument>
        </arguments>
    </type>
    <type name="BabaYaga\Vendor\Model\ResourceModel\Vendor\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">vendor_entity</argument>
            <argument name="eventPrefix" xsi:type="string">baba_yaga_vendor_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">baba_yaga_vendor_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">BabaYaga\Vendor\Model\ResourceModel\Vendor</argument>
        </arguments>
    </type>
    <type name="Magento\Framework\App\AreaList">
        <arguments>
            <argument name="areas" xsi:type="array">
                <item name="marketplace" xsi:type="array">
                    <item name="frontName" xsi:type="string">marketplace</item>
                </item>
            </argument>
        </arguments>
    </type>
</config>
